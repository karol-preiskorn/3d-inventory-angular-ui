{
  "name": "3d-inventory-angular-ui",
  "version": "0.3.30",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "clear:dist": "rm -rf dist",
    "clear:all": "npm run clear:cache && npm run clear:dist",
    "clear:cache": "rm -rf .angular/cache",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "cp": "cp -f README.md src/assets",
    "prepare": "husky install",
    "prettier": "prettier --check src/**/*.ts",
    "server:build": "cp -f README.md src/assets && ng build --watch",
    "server:json": "cd backend && json-server --watch db_v2.json",
    "server:kill": "killall \"ng serve\"",
    "server:lite": "lite-server --baseDir='dist/3d-inventory-angular-ui/browser'",
    "server:ng": "npm run clear:all && npm run cp && ng serve  --watch",
    "test": "ng test",
    "test:nyc": "nyc --reporter=html --produce-source-map --reporter=text npm test",
    "update": "npm-check-updates -u && npm update && npm upgrade"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular/animations": "^16.1.7",
    "@angular/common": "^16.1.7",
    "@angular/compiler": "^16.1.7",
    "@angular/core": "^16.1.7",
    "@angular/forms": "^16.1.7",
    "@angular/material": "^16.1.6",
    "@angular/platform-browser": "^16.1.7",
    "@angular/platform-browser-dynamic": "^16.1.7",
    "@angular/platform-server": "^16.1.7",
    "@angular/router": "^16.1.7",
    "@compodoc/compodoc": "^1.1.21",
    "@faker-js/faker": "^8.0.2",
    "@ng-bootstrap/ng-bootstrap": "^15.1.0",
    "@nguniversal/express-engine": "^16.1.1",
    "@types/lodash": "^4.14.196",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "depcheck": "^1.4.3",
    "express": "^4.18.2",
    "https-browserify": "^1.0.0",
    "install": "^0.13.0",
    "jquery": "^3.7.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.3",
    "karma-cli": "^2.0.0",
    "karma-typescript": "^5.5.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "nav-bar": "^1.0.3",
    "ngx-bootstrap": "^11.0.2",
    "ngx-markdown": "^16.0.0",
    "ngx-pagination": "^6.0.3",
    "node-sass": "^9.0.0",
    "npm-check-updates": "^16.10.17",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "three": "^0.155.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "ts-sync-request": "^1.4.1",
    "tslib": "^2.6.1",
    "winston": "^3.10.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular/cli": "^16.1.6",
    "@angular/compiler-cli": "^16.1.7",
    "@angular/localize": "^16.1.7",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.44.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.5",
    "@types/jquery": "^3.5.16",
    "@types/json-server": "^0.14.4",
    "@types/karma-coverage": "^2.0.1",
    "@types/karma-jasmine": "^4.0.2",
    "@types/karma-jsdom-launcher": "^8.0.2",
    "@types/node": "^20.4.5",
    "@types/node-sass": "^4.11.3",
    "@types/path-browserify": "^1.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/three": "^0.154.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "angular-cli-ghpages": "^1.0.6",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-jsdom-launcher": "^15.0.0",
    "lint-staged": "^13.2.3",
    "lite-server": "^2.6.1",
    "ng-packagr": "^16.1.0",
    "nyc": "^15.1.0",
    "prettier": "3.0.0",
    "typescript": "^5.1.6",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0"
  }
}
