    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "browserify-zlib": "^0.2.0",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "depcheck": "^1.4.7",
    "eslint-scope": "^8.0.2",
    "express": "^4.21.0",
    "https-browserify": "^1.0.0",
    "install": "^0.13.0",
