{
  "name": "3d-inventory-angular-ui",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "version": "0.3.4",
  "private": true,
  "scripts": {
    "update": "npm update && npm upgrade",
    "build": "cp -f README.md src/assets && ng build --watch --cross-origin anonymous --source-map true",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "kill-ng": "killall \"ng serve\"",
    "prepare": "husky install",
    "prettier": "prettier --check src/**/*.ts",
    "start:all": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" --prefix \"{time}-{pid}\"  \"npm:start:ng\" \"npm:start:json-server\" --kill-others",
    "start:json-server": "cd backend && json-server --watch db_v2.json",
    "start": "ng serve --poll=2000  --watch",
    "test": "ng test",
    "test:nyc": "nyc --reporter=html --produce-source-map --reporter=text npm test",
    "test:e2e": "ng e2e",
    "prerender": "ng run 3d-inventory-angular-ui:prerender"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/platform-server": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@compodoc/compodoc": "^1.1.21",
    "@ng-bootstrap/ng-bootstrap": "^14.1.1",
    "@nguniversal/express-engine": "^15.2.1",
    "@types/lodash": "^4.14.195",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.5",
    "chalk": "^5.2.0",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "jquery": "^3.7.0",
    "jsdom": "^21.1.2",
    "json-server": "^0.17.3",
    "karma-cli": "^2.0.0",
    "karma-typescript": "^5.5.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "nav-bar": "^1.0.3",
    "ngx-bootstrap": "^10.3.0",
    "ngx-markdown": "^15.1.2",
    "ngx-pagination": "^6.0.3",
    "node-sass": "^9.0.0",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "rxjs": "~7.8.1",
    "three": "^0.152.2",
    "three-orbitcontrols-ts": "^0.1.2",
    "tslib": "^2.5.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "^15.2.7",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/localize": "^15.2.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@nguniversal/builders": "^15.2.1",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.40.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.2",
    "@types/jquery": "^3.5.16",
    "@types/json-server": "^0.14.4",
    "@types/karma-coverage": "^2.0.1",
    "@types/karma-jasmine": "^4.0.2",
    "@types/karma-jsdom-launcher": "^8.0.2",
    "@types/marked": "^4.0.3",
    "@types/node": "^20.2.5",
    "@types/node-sass": "^4.11.3",
    "@types/path-browserify": "^1.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/three": "^0.152.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-jsdom-launcher": "^14.0.0",
    "lint-staged": "^13.2.2",
    "ng-packagr": "^15.2.2",
    "nyc": "^15.1.0",
    "prettier": "2.8.8",
    "typescript": "<5.0.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0",
    "vite": "^4.3.8"
  }
}
