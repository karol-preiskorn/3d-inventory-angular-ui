{
  "name": "3d-inventory-angular-ui",
  "version": "0.3.38",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "build": "ng build 3d-inventory-angular-ui",
    "clear:all": "npm run clear:cache && npm run clear:dist && npm run clear:node_install",
    "clear:cache": "rm -rf .angular/cache",
    "clear:dist": "rm -rf dist",
    "clear:node_install": "rm -rf node_modules",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "cp": "cp -f README.md src/assets",
    "github:issues": "MAX_NUM=`gh issue list --limit 1 --json number | jq \".[].number\"`; for n in `seq 1 $MAX_NUM`; do gh issue view $n --json assignees,author,body,closed,closedAt,comments,createdAt,id,labels,milestone,number,projectCards,reactionGroups,state,title,updatedAt,url >> github-dump.json; done",
    "github:issues2": "gh issue list --limit 10000 --state open --json number,title,assignees,state,url | jq  -r '[\"number\",\"title\",\"assignees\",\"state\",\"url\"], (.[] | [.number, .titlex!, (.assignees | if .|length==0 then \"Unassigned\" elif .|length>1 then map(.login)|join(\",\") else .[].login end) , .state, .url]) | @tsv' > issues-$(date '+%Y-%m-%d').tsv",
    "podman:build": "podman build -t 3d-inventory:0.1 .",
    "podman:clear": "podman rmi --all --force",
    "podman:run": "podman run -p 4000:80 --name 3d-inventory 3d-inventory:0.1",
    "podman:tag": "podman tag 3d-inventory:0.1 eu.gcr.io/bloggyboots/app:0.1",
    "prettier": "prettier --check src/**/*.ts",
    "server:json": "cd backend && json-server --watch db_v2.json",
    "server:kill": "killall \"ng serve\"",
    "server:lite": "lite-server --baseDir='dist/3d-inventory-angular-ui/browser'",
    "server:ng": "npm run clear:all && npm run cp && ng serve --host 0.0.0.0  --watch",
    "server:run": "npm run cp && ng build --watch",
    "start": "concurrently --kill-others \"npm run server:ng\" \"npm run server:json\"",
    "test": "ng test",
    "test:nyc": "nyc --reporter=html --produce-source-map --reporter=text npm test",
    "update": "npm-check-updates -u && npm update && npm upgrade"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^17.0.9",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@aws-amplify/cli": "^12.8.2",
    "@compodoc/compodoc": "^1.1.23",
    "@faker-js/faker": "^8.3.1",
    "@ng-bootstrap/ng-bootstrap": "^15.0.0",
    "@types/lodash": "^4.14.202",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "depcheck": "^1.4.7",
    "express": "^4.18.2",
    "https-browserify": "^1.0.0",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.4",
    "karma-cli": "^2.0.0",
    "karma-typescript": "^5.5.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "nav-bar": "^1.0.3",
    "ngx-bootstrap": "^11.0.2",
    "ngx-markdown": "^16.0.0",
    "ngx-pagination": "^6.0.3",
    "node-gyp": "^10.0.1",
    "node-sass": "^9.0.0",
    "npm-check-updates": "^16.14.11",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "rxjs": "~7.8.1",
    "three": "^0.158.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "ts-sync-request": "^1.4.1",
    "tslib": "^2.6.2",
    "winston": "^3.11.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
