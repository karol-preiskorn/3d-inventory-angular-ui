{
  "name": "3d-inventory-angular-ui",
  "version": "0.25.0",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "buffer": false,
    "process": false
  },
  "scripts": {
    "api": "cd ../3d-inventory-mongo-api && npm run start",
    "build": "npm --no-git-tag-version version patch && ng build 3d-inventory-angular-ui",
    "clear:all": "npm run clear:cache && npm run clear:dist && npm run clear:node_install",
    "clear:cache": "rm -rf .angular/cache",
    "clear:dist": "rm -rf dist",
    "clear:node_install": "rm -rf node_modules",
    "compodoc": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "cp": "cp -f README.md src/assets",
    "github:issues": "MAX_NUM=`gh issue list --limit 1 --json number | jq \".[].number\"`; for n in `seq 1 $MAX_NUM`; do gh issue view $n --json assignees,author,body,closed,closedAt,comments,createdAt,id,labels,milestone,number,projectCards,reactionGroups,state,title,updatedAt,url >> github-dump.json; done",
    "github:issues2": "gh issue list --limit 10000 --state open --json number,title,assignees,state,url | jq  -r '[\"number\",\"title\",\"assignees\",\"state\",\"url\"], (.[] | [.number, .titlex!, (.assignees | if .|length==0 then \"Unassigned\" elif .|length>1 then map(.login)|join(\",\") else .[].login end) , .state, .url]) | @tsv' > issues-$(date '+%Y-%m-%d').tsv",
    "kill-ng-serve": "killall \"ng serve\"",
    "kill-port-usage-4200": "kill -s 9 `lsof -nP -iTCP:4200 -sTCP:LISTEN | sed -n 2p | awk '{print $2}'`",
    "kill-port-usage-8080": "kill -s 9 `lsof -nP -iTCP:8080 -sTCP:LISTEN | sed -n 2p | awk '{print $2}'`",
    "lint:eslint": "eslint --fix src",
    "lint:prettier": "prettier --write src/**/*.ts",
    "podman:build": "podman build -t 3d-inventory:0.1 .",
    "podman:clear": "podman rmi --all --force",
    "podman:run": "podman run -p 4000:80 --name 3d-inventory 3d-inventory:0.1",
    "podman:tag": "podman tag 3d-inventory:0.1 eu.gcr.io/bloggyboots/app:0.1",
    "pre-commit": "lint-staged",
    "start": "npm --no-git-tag-version version patch && npm run cp && ng serve --watch",
    "test": "ng test",
    "test:nyc": "nyc --reporter=html --produce-source-map --reporter=text npm test",
    "update": "npm-check-updates -u && npm update && npm upgrade",
    "a": " eslint --report-unused-disable-directives --config eslint.config.js --fix src",
    "lint": "ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write --ignore-unknown src/**/*.ts",
      "eslint --fix src",
      "git add -A ."
    ],
    "src/**/*.html": [
      "prettier --write --ignore-unknown src/**/*.ts",
      "git add -A ."
    ]
  },
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/platform-server": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@compodoc/compodoc": "^1.1.25",
    "@faker-js/faker": "^8.4.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@types/lodash": "^4.17.5",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "depcheck": "^1.4.7",
    "eslint-scope": "^8.0.1",
    "express": "^4.19.2",
    "https-browserify": "^1.0.0",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "jsdom": "^24.1.0",
    "karma-cli": "^2.0.0",
    "karma-typescript": "^5.5.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "nav-bar": "^1.0.3",
    "ngx-bootstrap": "^12.0.0",
    "ngx-markdown": "^17.0.0",
    "ngx-pagination": "^6.0.3",
    "node-gyp": "^10.1.0",
    "node-sass": "^9.0.0",
    "npm-check-updates": "^16.14.20",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.7",
    "rxjs": "~7.8.1",
    "three": "^0.165.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "ts-sync-request": "^1.4.1",
